{
  "name": "myprojectname",
  "version": "0.0.1",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "android": "yarn workspace mobile android",
    "compile": "tsc -b --incremental",
    "ios": "yarn workspace mobile ios",
    "studio": "yarn workspace mobile studio",
    "xcode": "yarn workspace mobile xcode",
    "lint": "eslint \"packages/**/*.{js,ts,tsx}\"",
    "lint:fix": "eslint \"packages/**/*.{js,ts,tsx}\" --quiet --fix",
    "test": "yarn workspaces run test",
    "docs": "jsdoc -c jsdoc.conf.json"
  },
  "dependencies": {
    "react-native": "*"
  },
  "devDependencies": {
    "axios": "^0.19.2",
    "better-docs": "2.3.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.2.5",
    "jsdoc": "3.6.5",
    "jsdoc-plugin-typescript": "2.0.5",
    "modal-enhanced-react-native-web": "^0.2.0",
    "prettier": "2.0.5",
    "styled-components": "^5.1.1",
    "typescript": "3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:fix"
    }
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
